extends ../layout

block content
  .container
    h1.main-title 신고가 들어온 글
          
    br
    if compInfos
      table.table.table-hover
        thead
          tr
            th 제목
            th 주최사
            th 연락처
            th 신고 횟수
        tbody
            each compInfo in compInfos
              tr
                td
                  a(href=`/compInfos/${compInfo._id}`)= compInfo.title
                td= compInfo.host
                td= compInfo.contact
                td= compInfo.off
    else
        p 신고가 들어온 글이 없습니다.